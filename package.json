{
  "name": "mean-book-ui",
  "version": "1.0.0",
  "description": "MEAN book code UI",
  "main": "index.js",
  "scripts": {
    "#start": "UI server. HMR is enabled in dev mode.",
    "start": "node dist/server.js",
    "#lint": "Runs ESLint on all relevant files",
    "lint": "eslint server src browser --ext js,jsx",
    "#compile": "Generates JS bundles for production. Use with start.",
    "compile": "webpack --mode production",
    "#watch": "Compile, and recompile on any changes.",
    "##watch": "webpack --watch",
    "#watch-server-hmr": "Recompile server HMR bundle on changes.",
    "watch-server-hmr": "webpack -w --config webpack.serverHMR.js",
    "#dev-all": "Dev mode: watch for server changes and start UI server",
    "dev-all": "rm dist/* && npm run watch-server-hmr & sleep 5 && npm start",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ubuntumaroon/MEAN-book.git"
  },
  "keywords": [
    "MEAN"
  ],
  "author": "feng",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ubuntumaroon/MEAN-book/issues"
  },
  "homepage": "https://github.com/ubuntumaroon/MEAN-book#readme",
  "dependencies": {
    "@ungap/url-search-params": "github:ungap/url-search-params",
    "bootstrap": "^4.5.0",
    "core-js": "^3.6.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "http-proxy-middleware": "^1.0.4",
    "isomorphic-fetch": "^2.2.1",
    "jquery": "^3.5.1",
    "nodemon": "^2.0.4",
    "popper.js": "^1.16.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-bootstrap": "^1.0.1",
    "react-dom": "^16.13.1",
    "react-icons": "^3.10.0",
    "react-router-bootstrap": "^0.25.0",
    "react-router-dom": "^5.2.0",
    "regenerator-runtime": "^0.13.5",
    "source-map-support": "^0.5.19"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "babel-loader": "^8.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2"
  }
}
